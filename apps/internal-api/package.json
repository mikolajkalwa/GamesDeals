{
  "name": "internal-api",
  "version": "2.1.2",
  "main": "dist/main.js",
  "private": true,
  "devDependencies": {
    "@nestjs/cli": "^10.1.10",
    "@nestjs/testing": "^10.1.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "eslint": "^8.46.0",
    "eslint-config-games-deals": "workspace:*",
    "jest": "^29.6.2",
    "jest-mock": "^29.6.2",
    "prettier": "^3.0.0",
    "prettier-config": "workspace:*",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.1.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.2",
    "@nestjs/platform-express": "^10.1.2",
    "@nestjs/terminus": "^10.0.1",
    "@prisma/client": "^5.0.0",
    "bullmq": "^4.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.9.2",
    "nestjs-pino": "^3.3.0",
    "pino-http": "^8.3.3",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "scripts": {
    "build": "nest build",
    "build:prod": "nest build --path ./tsconfig.prod.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint",
    "test": "jest",
    "test:e2e": "jest --runInBand --config ./jest-e2e.config.json",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate"
  },
  "prettier": "prettier-config",
  "lint-staged": {
    "*.ts": [
      "pnpm --filter insternal-api exec prettier --list-different",
      "pnpm --filter insternal-api exec eslint"
    ]
  }
}
