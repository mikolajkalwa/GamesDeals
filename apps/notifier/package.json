{
  "name": "notifier",
  "version": "4.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "bullmq": "^4.6.3",
    "close-with-grace": "^1.2.0",
    "dotenv": "^16.3.1",
    "gd-api-client": "workspace:*",
    "pino": "^8.14.2",
    "undici": "^5.22.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-games-deals": "workspace:*",
    "prettier": "^3.0.0",
    "prettier-config": "workspace:*",
    "rimraf": "^5.0.1",
    "tsconfig": "workspace:*",
    "typescript": "5.1.6"
  },
  "prettier": "prettier-config",
  "lint-staged": {
    "*.ts": [
      "pnpm --filter notifier exec prettier --list-different",
      "pnpm --filter notifier exec eslint"
    ]
  }
}
