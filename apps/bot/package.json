{
  "private": true,
  "name": "bot",
  "version": "2.1.5",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "start:prod": "node dist/index.js",
    "lint": "eslint"
  },
  "dependencies": {
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "got": "^11.8.6",
    "pino": "^8.15.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-games-deals": "workspace:*",
    "prettier": "^3.0.1",
    "prettier-config": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  },
  "prettier": "prettier-config",
  "lint-staged": {
    "*.ts": [
      "pnpm --filter bot exec prettier --check",
      "pnpm --filter bot exec eslint"
    ]
