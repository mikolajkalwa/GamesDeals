{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": [
        "jest"
    ],
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0
    }
}